# GitHub Configuration
GITHUB_TOKEN=your_github_personal_access_token_here
GITHUB_OWNER=your_github_username
GITHUB_REPO=your_repository_name

# GitHub Copilot (如果使用独立 API)
COPILOT_API_KEY=your_copilot_api_key_here

# ANSYS Fluent Configuration
FLUENT_PATH=C:/Program Files/ANSYS Inc/v241/fluent/ntbin/win64/fluent.exe
FLUENT_VERSION=2024R1
FLUENT_PRECISION=dp
FLUENT_DIMENSION=3d

# MCP Server Configuration
MCP_SERVER_PORT=3000
MCP_SERVER_HOST=localhost

# OpenAI Configuration (用于 Copilot 替代方案)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/fluent_copilot.log

# Workspace
WORKSPACE_PATH=./workspace
TEMP_PATH=./temp
